{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Dagster: debug with .debug.py",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}/orchestration",
      "module": "debug",
      "justMyCode": false,
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "DAGSTER_DBT_PARSE_PROJECT_ON_LOAD": "YES"
      }
    },
    {
      "name": "Dagster: Debug Dagit UI",
      "type": "python",
      "request": "launch",
      "module": "dagster",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/orchestration",
      "args": [
        "dev",
      ],
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "DAGSTER_DBT_PARSE_PROJECT_ON_LOAD": "YES"
      }
    }
  ]
}